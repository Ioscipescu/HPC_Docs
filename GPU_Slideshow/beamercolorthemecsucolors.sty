%
% ====================
% Theme
% ====================


\setbeamercolor{structure}{fg=black,bg=csugreen}

%Slide Titles
\setbeamercolor{titlelike}{fg=white,bg=csugreen}

%% Standard Block
\setbeamercolor{block title}{fg=white,bg=csugreen}
\setbeamercolor{block body}{fg=black,bg=loverslane}

%% Alert Block
\setbeamercolor{block title alerted}{fg=white,bg=csugreen}
\setbeamercolor{block body alerted}{fg=black,bg=csugold}

%% Example Block
\setbeamercolor{block title example}{fg=white,bg=csugreen}
\setbeamercolor{block body example}{fg=black,bg=energygreen}

%% Header Footer
\setbeamercolor{section in head/foot}{bg=aggieorange,fg=aggieorange}
\setbeamercolor{subsection in head/foot}{bg=aggieorange,fg=aggieorange}
\setbeamercolor{author in head/foot}{bg=aggieorange,fg=aggieorange}
\setbeamercolor{title in head/foot}{bg=aggieorange,fg=aggieorange}

\setbeamercolor{headline}{bg=aggieorange,fg=aggieorange}
\setbeamercolor{footline}{bg=aggieorange,fg=aggieorange}
\setbeamercolor{institute in head/foot}{bg=aggieorange,fg=aggieorange}